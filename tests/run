    # "$ROOT/densenet/execute $ROOT/densenet/normal/config_lsa16.conf --engine maml --weights $lsa16_maml -o $ROOT/results"
    # "$ROOT/densenet/execute $ROOT/densenet/normal/config_ciarp.conf --engine maml --weights $ciarp_maml -o $ROOT/results"
    # "$ROOT/densenet/execute $ROOT/densenet/normal/config_rwth.conf --engine maml --weights $rwth_maml -o $ROOT/results"

    # "$ROOT/densenet/execute $ROOT/densenet/normal/config_lsa16_split_5.conf $ROOT/densenet/normal/config_lsa16_split_10.conf $ROOT/densenet/normal/config_lsa16_split_15.conf $ROOT/densenet/normal/config_lsa16_split_20.conf -o $ROOT/results"
