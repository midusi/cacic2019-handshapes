    # "$ROOT/densenet/execute $ROOT/densenet/normal/config_lsa16.conf --weights $lsa16 -o $ROOT/results"
    # "$ROOT/densenet/execute $ROOT/densenet/normal/config_ciarp.conf --weights $ciarp -o $ROOT/results"
    # "$ROOT/densenet/execute $ROOT/densenet/normal/config_rwth.conf --weights $rwth -o $ROOT/results"

