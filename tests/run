#!/usr/bin/env bash

ROOT=$(dirname $0)

ciarp="./results/ciarp/densenet/checkpoints/checkpoint.ciarp_densenet_test_growth_rate_64_layers_6_12.h5"
cifar10="./results/cifar10/densenet/checkpoints/checkpoint.cifar10_densenet_test_growth_rate_64_layers_6_12.h5"
lsa16="./results/lsa16/densenet/checkpoints/checkpoint.lsa16_densenet_test_growth_rate_64_layers_6_12.h5"
mnist="./results/mnist/densenet/checkpoints/checkpoint.mnist_densenet_test_growth_rate_64_layers_6_12.h5"
rwth="./results/rwth/densenet/checkpoints/checkpoint.rwth_densenet_test_growth_rate_64_layers_6_12.h5"

ciarp_maml="./results/ciarp/densenet_maml/checkpoints/checkpoint.ciarp_densenet_test_growth_rate_64_layers_6_12.h5"
cifar10_maml="./results/cifar10/densenet_maml/checkpoints/checkpoint.cifar10_densenet_test_growth_rate_64_layers_6_12.h5"
lsa16_maml="./results/lsa16/densenet_maml/checkpoints/checkpoint.lsa16_densenet_test_growth_rate_64_layers_6_12.h5"
mnist_maml="./results/mnist/densenet_maml/checkpoints/checkpoint.mnist_densenet_test_growth_rate_64_layers_6_12.h5"
rwth_maml="./results/rwth/densenet_maml/checkpoints/checkpoint.rwth_densenet_test_growth_rate_64_layers_6_12.h5"

tl_ciarp_weights=( $cifar10 $cifar10_maml $mnist $mnist_maml $lsa16 $lsa16_maml $rwth $rwth_maml )
tl_lsa16_weights=( $ciarp $ciarp_maml $cifar10 $cifar10_maml $mnist $mnist_maml $rwth $rwth_maml )
tl_rwth_weights=( $ciarp $ciarp_maml $cifar10 $cifar10_maml $lsa16 $lsa16_maml $mnist $mnist_maml )

tl_ciarp_weights=$(printf ",%s" "${tl_ciarp_weights[@]}")
tl_lsa16_weights=$(printf ",%s" "${tl_lsa16_weights[@]}")
tl_rwth_weights=$(printf ",%s" "${tl_rwth_weights[@]}")

tl_ciarp_weights=${tl_ciarp_weights:1}
tl_lsa16_weights=${tl_lsa16_weights:1}
tl_rwth_weights=${tl_rwth_weights:1}

