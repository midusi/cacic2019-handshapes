#!/usr/bin/env bash

ROOT=$(dirname $0)

S=1
N=1
T=5

tests=()

cifar10="./results/cifar10/densenet/checkpoints/checkpoint.cifar10_densenet_test_growth_rate_64_layers_6_12.h5"
mnist="./results/mnist/densenet/checkpoints/checkpoint.mnist_densenet_test_growth_rate_64_layers_6_12.h5"

cifar10_maml="./results/cifar10/densenet_maml/checkpoints/checkpoint.cifar10_densenet_test_growth_rate_64_layers_6_12.h5"
mnist_maml="./results/1_mnist/densenet_maml/checkpoints/checkpoint.mnist_densenet_test_growth_rate_64_layers_6_12.h5"

for i in $(seq $S $N); do
    protonet_split_tests=(
        "$ROOT/protonet/execute $ROOT/protonet/config_lsa16.conf --times $T --split full_75_25_${i} -o $ROOT/results/results_protonet_${i}_lsa16"
        "$ROOT/protonet/execute $ROOT/protonet/config_ciarp.conf --times $T --split full_75_25_${i} -o $ROOT/results/results_protonet_${i}_ciarp"
        "$ROOT/protonet/execute $ROOT/protonet/config_rwth.conf --times $T --split full_75_25_${i} -o $ROOT/results/results_protonet_${i}_rwth"

        "$ROOT/protonet/execute $ROOT/protonet/config_lsa16_split_5.conf $ROOT/protonet/config_lsa16_split_10.conf $ROOT/protonet/config_lsa16_split_15.conf $ROOT/protonet/config_lsa16_split_20.conf $ROOT/protonet/config_lsa16_split_30.conf --times $T --is-split -o $ROOT/results/results_protonet_${i}_lsa16"
        "$ROOT/protonet/execute $ROOT/protonet/config_ciarp_split_5.conf $ROOT/protonet/config_ciarp_split_10.conf $ROOT/protonet/config_ciarp_split_15.conf $ROOT/protonet/config_ciarp_split_20.conf $ROOT/protonet/config_ciarp_split_30.conf $ROOT/protonet/config_ciarp_split_40.conf $ROOT/protonet/config_ciarp_split_100.conf --times $T --is-split -o $ROOT/results/results_protonet_${i}_ciarp"
        "$ROOT/protonet/execute $ROOT/protonet/config_rwth_split_5.conf $ROOT/protonet/config_rwth_split_10.conf $ROOT/protonet/config_rwth_split_15.conf $ROOT/protonet/config_rwth_split_20.conf $ROOT/protonet/config_rwth_split_30.conf $ROOT/protonet/config_rwth_split_40.conf $ROOT/protonet/config_rwth_split_100.conf --times $T --is-split -o $ROOT/results/results_protonet_${i}_rwth"
    )

    # tests=( "${tests[@]}" "${protonet_split_tests[@]}" )
done

densenet_tests=(
    # "$ROOT/densenet/execute $ROOT/densenet/normal/config_cifar10.conf --weights $cifar10 --times $T --split full_75_25_${i} -o $ROOT/results/results_densenet_${i}"
    # "$ROOT/densenet/execute $ROOT/densenet/normal/config_mnist.conf --weights $mnist --times $T --split full_75_25_${i} -o $ROOT/results/results_densenet_${i}"

    # "$ROOT/densenet/execute $ROOT/densenet/maml/config_mnist.conf --engine maml --weights $mnist_maml --times $T --split full_75_25_${i} -o $ROOT/results/results_densenet_${i}"
    "$ROOT/densenet/execute $ROOT/densenet/maml/config_cifar10.conf --engine maml --weights $cifar10_maml --times $T --split full_75_25_${i} -o $ROOT/results/results_densenet_${i}"
)

#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
# Transfer Learning & Model Agnostic Meta Learning experiments using DenseNet
#~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~
for i in $(seq $S $N); do
    ciarp="./results/${i}_ciarp/densenet/checkpoints/checkpoint.ciarp_densenet_test_growth_rate_64_layers_6_12.h5"
    lsa16="./results/${i}_lsa16/densenet/checkpoints/checkpoint.lsa16_densenet_test_growth_rate_64_layers_6_12.h5"
    rwth="./results/${i}_rwth/densenet/checkpoints/checkpoint.rwth_densenet_test_growth_rate_64_layers_6_12.h5"

    ciarp_maml="./results/${i}_ciarp/densenet_maml/checkpoints/checkpoint.ciarp_densenet_test_growth_rate_64_layers_6_12.h5"
    lsa16_maml="./results/${i}_lsa16/densenet_maml/checkpoints/checkpoint.lsa16_densenet_test_growth_rate_64_layers_6_12.h5"
    rwth_maml="./results/${i}_rwth/densenet_maml/checkpoints/checkpoint.rwth_densenet_test_growth_rate_64_layers_6_12.h5"

    # tl_ciarp_weights=( $cifar10 $cifar10_maml $mnist $mnist_maml $lsa16 $lsa16_maml $rwth $rwth_maml )
    # tl_lsa16_weights=( $ciarp_maml $cifar10 $cifar10_maml $mnist $mnist_maml $rwth $rwth_maml )
    # tl_rwth_weights=( $ciarp_maml $cifar10 $cifar10_maml $lsa16 $lsa16_maml $mnist $mnist_maml )

    tl_ciarp_weights=( $cifar10_maml )
    tl_lsa16_weights=( $cifar10_maml )
    tl_rwth_weights=( $cifar10_maml )

    tl_ciarp_weights=$(printf ",%s" "${tl_ciarp_weights[@]}"); tl_ciarp_weights=${tl_ciarp_weights:1}
    tl_lsa16_weights=$(printf ",%s" "${tl_lsa16_weights[@]}"); tl_lsa16_weights=${tl_lsa16_weights:1}
    tl_rwth_weights=$(printf ",%s" "${tl_rwth_weights[@]}"); tl_rwth_weights=${tl_rwth_weights:1}

    densenet_split_tests=(
        # "$ROOT/densenet/execute $ROOT/densenet/normal/config_lsa16.conf --weights $lsa16 --times $T --split full_75_25_${i} -o $ROOT/results/results_densenet_${i}_lsa16"
        # "$ROOT/densenet/execute $ROOT/densenet/normal/config_ciarp.conf --weights $ciarp --times $T --split full_75_25_${i} -o $ROOT/results/results_densenet_${i}_ciarp"
        # "$ROOT/densenet/execute $ROOT/densenet/normal/config_rwth.conf --weights $rwth --times $T --split full_75_25_${i} -o $ROOT/results/results_densenet_${i}_rwth"

        # "$ROOT/densenet/execute $ROOT/densenet/maml/config_lsa16.conf --engine maml --weights $lsa16_maml --times $T --split full_75_25_${i} -o $ROOT/results/results_densenet_maml_${i}_lsa16"
        # "$ROOT/densenet/execute $ROOT/densenet/maml/config_ciarp.conf --engine maml --weights $ciarp_maml --times $T --split full_75_25_${i} -o $ROOT/results/results_densenet_maml_${i}_ciarp"
        # "$ROOT/densenet/execute $ROOT/densenet/maml/config_rwth.conf --engine maml --weights $rwth_maml --times $T --split full_75_25_${i} -o $ROOT/results/results_densenet_maml_${i}_rwth"

        "$ROOT/transfer_learning/execute $ROOT/transfer_learning/normal/config_lsa16.conf --weights $tl_lsa16_weights --times $T -o $ROOT/results/results_densenet_${i}_lsa16"
        "$ROOT/transfer_learning/execute $ROOT/transfer_learning/normal/config_ciarp.conf --weights $tl_ciarp_weights --times $T -o $ROOT/results/results_densenet_${i}_ciarp"
        "$ROOT/transfer_learning/execute $ROOT/transfer_learning/normal/config_rwth.conf --weights $tl_rwth_weights --times $T -o $ROOT/results/results_densenet_${i}_rwth"

        "$ROOT/transfer_learning/execute $ROOT/transfer_learning/maml/config_lsa16.conf --engine maml --weights $tl_lsa16_weights --times $T -o $ROOT/results/results_densenet_maml_${i}_lsa16"
        "$ROOT/transfer_learning/execute $ROOT/transfer_learning/maml/config_ciarp.conf --engine maml --weights $tl_ciarp_weights --times $T -o $ROOT/results/results_densenet_maml_${i}_ciarp"
        "$ROOT/transfer_learning/execute $ROOT/transfer_learning/maml/config_rwth.conf --engine maml --weights $tl_rwth_weights --times $T -o $ROOT/results/results_densenet_maml_${i}_rwth"

        # "$ROOT/densenet/execute $ROOT/densenet/normal/config_lsa16_split_5.conf $ROOT/densenet/normal/config_lsa16_split_10.conf $ROOT/densenet/normal/config_lsa16_split_15.conf $ROOT/densenet/normal/config_lsa16_split_20.conf --times $T --is-split -o $ROOT/results/results_densenet_${i}_lsa16"
        # "$ROOT/densenet/execute $ROOT/densenet/normal/config_ciarp_split_5.conf $ROOT/densenet/normal/config_ciarp_split_10.conf $ROOT/densenet/normal/config_ciarp_split_15.conf $ROOT/densenet/normal/config_ciarp_split_20.conf $ROOT/densenet/normal/config_ciarp_split_30.conf $ROOT/densenet/normal/config_ciarp_split_40.conf $ROOT/densenet/normal/config_ciarp_split_100.conf --times $T --is-split -o $ROOT/results/results_densenet_${i}_ciarp"
        # "$ROOT/densenet/execute $ROOT/densenet/normal/config_rwth_split_5.conf $ROOT/densenet/normal/config_rwth_split_10.conf $ROOT/densenet/normal/config_rwth_split_15.conf $ROOT/densenet/normal/config_rwth_split_20.conf $ROOT/densenet/normal/config_rwth_split_30.conf $ROOT/densenet/normal/config_rwth_split_40.conf --times $T --is-split -o $ROOT/results/results_densenet_${i}_rwth"
        
        # "$ROOT/densenet/execute $ROOT/densenet/maml/config_lsa16_split_5.conf $ROOT/densenet/maml/config_lsa16_split_10.conf $ROOT/densenet/maml/config_lsa16_split_15.conf $ROOT/densenet/maml/config_lsa16_split_20.conf --engine maml --times $T --is-split -o $ROOT/results/results_densenet_maml_${i}_lsa16"
        # "$ROOT/densenet/execute $ROOT/densenet/maml/config_ciarp_split_5.conf $ROOT/densenet/maml/config_ciarp_split_10.conf $ROOT/densenet/maml/config_ciarp_split_15.conf $ROOT/densenet/maml/config_ciarp_split_20.conf $ROOT/densenet/maml/config_ciarp_split_30.conf $ROOT/densenet/maml/config_ciarp_split_40.conf $ROOT/densenet/maml/config_ciarp_split_100.conf --engine maml --times $T --is-split -o $ROOT/results/results_densenet_maml_${i}_ciarp"
        # "$ROOT/densenet/execute $ROOT/densenet/maml/config_rwth_split_5.conf $ROOT/densenet/maml/config_rwth_split_10.conf $ROOT/densenet/maml/config_rwth_split_15.conf $ROOT/densenet/maml/config_rwth_split_20.conf $ROOT/densenet/maml/config_rwth_split_30.conf $ROOT/densenet/maml/config_rwth_split_40.conf --engine maml --times $T --is-split -o $ROOT/results/results_densenet_maml_${i}_rwth"

        "$ROOT/transfer_learning/execute $ROOT/transfer_learning/normal/config_lsa16_split_5.conf $ROOT/transfer_learning/normal/config_lsa16_split_10.conf $ROOT/transfer_learning/normal/config_lsa16_split_15.conf $ROOT/transfer_learning/normal/config_lsa16_split_20.conf --weights $tl_lsa16_weights --times $T --is-split -o $ROOT/results/results_densenet_${i}_lsa16"
        "$ROOT/transfer_learning/execute $ROOT/transfer_learning/normal/config_ciarp_split_5.conf $ROOT/transfer_learning/normal/config_ciarp_split_10.conf $ROOT/transfer_learning/normal/config_ciarp_split_15.conf $ROOT/transfer_learning/normal/config_ciarp_split_20.conf $ROOT/transfer_learning/normal/config_ciarp_split_30.conf $ROOT/transfer_learning/normal/config_ciarp_split_40.conf $ROOT/transfer_learning/normal/config_ciarp_split_100.conf --weights $tl_ciarp_weights --times $T --is-split -o $ROOT/results/results_densenet_${i}_ciarp"
        "$ROOT/transfer_learning/execute $ROOT/transfer_learning/normal/config_rwth_split_5.conf $ROOT/transfer_learning/normal/config_rwth_split_10.conf $ROOT/transfer_learning/normal/config_rwth_split_15.conf $ROOT/transfer_learning/normal/config_rwth_split_20.conf $ROOT/transfer_learning/normal/config_rwth_split_30.conf $ROOT/transfer_learning/normal/config_rwth_split_40.conf --weights $tl_rwth_weights --times $T --is-split -o $ROOT/results/results_densenet_${i}_rwth"
    
        "$ROOT/transfer_learning/execute $ROOT/transfer_learning/maml/config_lsa16_split_5.conf $ROOT/transfer_learning/maml/config_lsa16_split_10.conf $ROOT/transfer_learning/maml/config_lsa16_split_15.conf $ROOT/transfer_learning/maml/config_lsa16_split_20.conf --engine maml --weights $tl_lsa16_weights --times $T --is-split -o $ROOT/results/results_densenet_maml_${i}_lsa16"
        "$ROOT/transfer_learning/execute $ROOT/transfer_learning/maml/config_ciarp_split_5.conf $ROOT/transfer_learning/maml/config_ciarp_split_10.conf $ROOT/transfer_learning/maml/config_ciarp_split_15.conf $ROOT/transfer_learning/maml/config_ciarp_split_20.conf $ROOT/transfer_learning/maml/config_ciarp_split_30.conf $ROOT/transfer_learning/maml/config_ciarp_split_40.conf $ROOT/transfer_learning/maml/config_ciarp_split_100.conf --engine maml --weights $tl_ciarp_weights --times $T --is-split -o $ROOT/results/results_densenet_maml_${i}_ciarp"
        "$ROOT/transfer_learning/execute $ROOT/transfer_learning/maml/config_rwth_split_5.conf $ROOT/transfer_learning/maml/config_rwth_split_10.conf $ROOT/transfer_learning/maml/config_rwth_split_15.conf $ROOT/transfer_learning/maml/config_rwth_split_20.conf $ROOT/transfer_learning/maml/config_rwth_split_30.conf $ROOT/transfer_learning/maml/config_rwth_split_40.conf --engine maml --weights $tl_rwth_weights --times $T --is-split -o $ROOT/results/results_densenet_maml_${i}_rwth"
    )

    tests=( "${tests[@]}" "${densenet_tests[@]}" "${densenet_split_tests[@]}" )
done

. $ROOT/runner.sh
